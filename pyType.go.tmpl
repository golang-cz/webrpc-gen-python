{{- define "pyType" -}}

{{- /* Used to validate types on server. */ -}}
{{- $type := .Type -}}
{{- $typeMap := .TypeMap -}}

{{- if isMapType .Type -}}
    dict

{{- else if isListType .Type -}}
    List[{{template "type" dict "Type" (listElemType .Type) "TypeMap" $typeMap}}]

{{- else if isStructType .Type -}}
    # {{.Type}}
    # TODO: Define Python structure here

{{- else -}}
    {{ get $typeMap .Type }}

{{- end -}}
{{- end -}}
